page{
  background-color: #faf9f7;
  border-top: solid 1rpx #d5d5d5;
}
.constainer_item{
  padding: 38rpx 20rpx;
}
/* 公共样式start */
/* * */
.identity_item,.identity_item_title,.identity_item_contact,.id_item_contact,.id_item_contact_mig,.id_mig_item{
  display: flex;
}
.id_item_contact,.id_item_contact_mig,.identity_item_contact{
  flex-direction: column;
  flex-wrap: wrap;
}
.id_mig_item_icon image{
  width: 100%;
  height: 100%;
}
/* * */
/* 公共样式end */
/* 标题start */
/* * */
.identity_item{
  width: 100%;
  flex-direction: column;
  font-size: 30rpx;
  color: #666666;
  background-color: #ffffff;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}
.identity_item_title{
  width: 100%;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  height: 80rpx;
  border-bottom: solid 1rpx #ececec;
}
.identity_item_title .tit{
  padding-left: 16rpx;
  font-weight: bold;
  color: #333333;
}
.identity_title_icon{
  width: 8rpx;
  height: 28rpx;
  background-color: #3889f0;
  border-radius: 8rpx;
  margin-left: 28rpx;
}
/* * */
/* 标题end */
/* 出租人身份证start */
.identity_item_contact{
  padding: 0 26rpx 5rpx 26rpx;
  font-size: 22rpx;
}
.id_item_contact_title{
  padding-bottom: 26rpx;
  padding-top: 40rpx;
}
.id_item_contact_mig{
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 35rpx;
}
.id_mig_item{
  width: 240rpx;
  height: 144rpx;
  background-color: #faf9f7;
  border-radius: 8rpx;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  overflow: hidden;
  /* padding-left: 24rpx; */
}
.id_mig_item image{
  width: 100%!important;
  height: 100%!important;
}
.id_mig_title{
  width: 126rpx;
}
.id_item_contact{
  border-bottom: solid 1rpx #e0e0e0;
}
.id_mig_item_icon{
  width: 40rpx;
  height: 30rpx;

}
/* 出租人身份证end */
/* 下一步按钮start */
.identity_next{
  width: 100%;
  padding: 20rpx 20rpx 38rpx 20rpx;
  margin-top: 28rpx;
}
.identity_next_btn::after{
  border: none;
}
wx-button{
  background-color: #3889f0;
  color: #ffffff;
  width: 100%;
  height: 90rpx;
  font-size: 32rpx;
}

.tenantsRelation image{
  width: 240rpx;
  height: 144rpx;
  border-radius: 8rpx;
  padding: 26rpx 0 35rpx 150rpx;
}

.msg{
	font-size: 22rpx;
	color: red;
	box-sizing: border-box;
  margin-left: 10rpx;
}