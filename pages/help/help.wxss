.container_items{
  width: 100%;
}
/* 重复样式start */
.help_item,.help_items,.help_item_name,.help_item_icon{
  display: flex;
}
.help_item_name image,.help_item_n_icon image,.help_item_icon image,.help_header image{
  width: 100%;
  height: 100%;
}
/* 重复样式end */
/* 顶部图片start */
.help_header{
  width: 100%;
  height: 200rpx;
}
.help_item_n_icon{
  width: 34rpx;
  height: 34rpx;
}
/* 顶部图片end */
/* 目录start */
.help_items{
  flex-direction: column;
  padding: 0 40rpx;
}
.help_item{
  justify-content: space-between;
  align-items: center;
  border-bottom: solid 1rpx #deecff;
  height: 102rpx;
}
.help_item_name{
  align-content: center;
  width: auto;
  justify-content: space-between;
}
.help_item_n_text{
  padding-left: 26rpx;
  font-size: 30rpx;
}
.help_item_icon{
  height: 30rpx;
  width: 30rpx;
  align-items: center;
}
/* 目录end */
